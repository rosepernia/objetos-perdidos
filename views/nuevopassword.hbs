
  <!--===============FORMULARIO DATOS OBJETO===============-->
<main>
<div class="container">
                <div class="row">
                    <div class="booking-form">
                        <div class="form-header">
                           
                        </div>
                        <form method="post" action="/usuarios/nuevousuario">
                                   <div class="col-sm-6">
                                    <div class="form-group">
                                        <span class="form-label">Nueva contraseña</span>
                                        <input class="form-control" type="password"
                                             name="password" required>
        
                                    </div>
                                </div>
                                
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <span class="form-label">Confirmar contraseña</span>
                                        <input class="form-control" type="password"
                                             name="password" required>
        
                                    </div>
                                </div>
                            
                                    <div class="form-btn">
                                        <button class="submit">Enviar</button>
                                    </div>

                                     <div class="col-8">
                                        <h3 class="confirm">{{mensaje}}</h3>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>


